{
  "workbench.startupEditor": "none",
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.fontSize": 16,
  "terminal.integrated.fontSize": 16,
  "debug.console.fontSize": 16,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.inlayHints.enabled": "off",
  "doxdocgen.c.commentPrefix": "///",
  "doxdocgen.c.triggerSequence": "///",
  "editor.defaultFormatter": "xaver.clang-format",
  "files.associations": {
    "new": "cpp",
    "string": "cpp",
    "atomic": "cpp",
    "memory_resource": "cpp",
    "chrono": "cpp",
    "*.tcc": "cpp",
    "sstream": "cpp",
    "deque": "cpp",
    "list": "cpp",
    "unordered_map": "cpp",
    "vector": "cpp",
    "iterator": "cpp",
    "string_view": "cpp",
    "fstream": "cpp",
    "functional": "cpp",
    "istream": "cpp",
    "ostream": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "algorithm": "cpp",
    "iosfwd": "cpp",
    "array": "cpp",
    "compare": "cpp",
    "ratio": "cpp",
    "type_traits": "cpp",
    "utility": "cpp",
    "limits": "cpp",
    "bitset": "cpp",
    "initializer_list": "cpp",
    "ranges": "cpp",
    "span": "cpp",
    "stacktrace": "cpp",
    "regex": "cpp",
    "valarray": "cpp",
    "charconv": "cpp",
    "variant": "cpp"
  },
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  }
}